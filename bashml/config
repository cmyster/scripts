# How many bots to spawn in each generation.
POPULATION=50

# When we hit this number, the simulation will end.
GENERATION=100

# How many bots do we want to use as a base to the next generation. We want
# this number to be multiply-able back to POPULATION.
GRADUATE=10

# Degree of % of change in the bot DNA between generation (1-100).
MUTATE=1
